  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "next build",
    "build-start": "next build && next start",
    "clean-codegen": "rimraf graphql/graphql.ts && rimraf graphql/graphql.schema.json",
    "codegen": "yarn clean-codegen && graphql-codegen --config codegen.yml",
    "codegen-watch": "yarn clean-codegen && graphql-codegen --config codegen.yml --watch",
    "dev": "next dev",
    "hasura-console": "cd ../hasura && hasura console --admin-secret=HASURA-ADMIN-SECRET-DEVELOPMENT",
    "lint": "next lint",
    "prettier": "prettier --write \"**/*.{ts,tsx}\" ",
    "prepare": "cd .. && husky install frontend/.husky",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc",
    "version-patch": "npm version patch",
    "version-minor": "npm version minor",
    "version-major": "npm version major"
  },
